package s21206_java30;
public class Car {
	
	static String s = "우리나라 자동차";	
	//필드 인스턴스 변수
	String company = "현대자동자";
	String model;
	String color;
	int maxSpeed;
	//생성자1,2,3,4
	Car(){
		//생성자 1
	}
	Car(String model){ //model 로컬변수	//생성자2
		this.model = model; //this.model 인스턴스		
	}
	Car(String model, String color){ 	//생성자3
		this.model = model;
		this.color = color;
	}
	Car(String model,String color,int maxSpeed){ 	//생성자4
		this.model = model; 			//공통실행코드	
		this.color = color;				//공통실행코드	
		this.maxSpeed = maxSpeed;		//공통실행코드	
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
